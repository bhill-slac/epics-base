#
# _APPNAME_ substitutions file
#

# Generate iocAdmin autosave request 
file autosave_iocAdmin.template
{
	{
		# Hack till msi supports empty substitutions
		IOC		=	$(IOC)
	}
}

# Generate EVR autosave request 
file autosave_evr.template
{
	{
		# Hack till msi supports empty substitutions
		IOC		=	$(IOC),
		EVR		=	$(EVR)
	}
}

